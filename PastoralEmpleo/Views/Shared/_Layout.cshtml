

<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <link rel="shortcut icon" href="assets/images/favicon.ico">

    <title>@ViewData["Title"] - PastoralEmpleo</title>

    <!--Morris Chart CSS -->
    <link rel="stylesheet" href="assets/plugins/morris/morris.css">

    <!-- App css -->
    <link href="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="~/css/style.css" rel="stylesheet" type="text/css" />
    <script src="~/lib/modernizr/modernizr.min.js"></script>

</head>

<body class="fixed-left">

    <!-- Begin page -->
    <div id="wrapper">

        <!-- Top Bar Start -->
        <div class="topbar">

            <!-- LOGO -->
            <div class="topbar-left">
                <a href="index.html" class="logo"><span>Pastoral<span>Empleo</span></span><i class="mdi mdi-layers"></i></a>
            </div>

            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container-fluid">

                    <!-- Page title -->


                    <nav class="navbar-custom">

                        <ul class="list-unstyled topbar-right-menu float-right mb-0">

                            <li>
                                <!-- Notification -->
                                <div class="notification-box">
                                    <ul class="list-inline mb-0">
                                        <li>
                                            <a href="javascript:void(0);" class="right-bar-toggle">
                                            </a>
                                            <div class="noti-dot">
                                                <span class="dot"></span>
                                                <span class="pulse"></span>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                                <!-- End Notification bar -->
                            </li>

                        </ul>
                    </nav>
                </div><!-- end container -->
            </div><!-- end navbar -->
        </div>
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">

                <!-- User -->
                <div class="user-box">

                    <figure class="full-box">
                        <img src="~/images/TeacherMaleAvatar2.png" />
                        <figcaption class="text-center text-titles">Mis datos</figcaption>
                    </figure>

                    
                </div>
                <!-- End User -->
                <!--- Sidemenu -->
                <div id="sidebar-menu">
                    <ul>
                        <li class="text-muted menu-title" style="color:#000000"><i class="fa fa-handshake-o"></i> BIENVENIDO</li>

                        <li>
                            <a asp-controller="Home" asp-action="Contact" style="color:#004D97"><i class="fa fa-spinner"></i><span> Panel de Control</span> </a>
                        </li>

                        <li>
                            <a asp-controller="Inscription" asp-action="Candidate" class="waves-effect" style="color:#004D97"><i class="fa fa-address-book"></i><span> Datos Personales</span></a>
                        </li>

                        <li>
                            <a asp-controller="Studies" asp-action="Studies" class="waves-effect" style="color:#004D97"><i class="fa fa-graduation-cap"></i><span> Formación </span> </a>
                        </li>

                        <li>
                            <a asp-controller="Experience" asp-action="Experience" class="waves-effect" style="color:#004D97"><i class="fa fa-black-tie"></i><span> Experiencia Laboral </span> </a>
                        </li>

                        <li>
                            <a asp-controller="Document" asp-action="Document" class="waves-effect" style="color:#004D97"><i class="fa fa-file-text"></i><span> Otros Documentos </span> </a>
                        </li>

                        <li>
                            <a asp-controller="Event" asp-action="Event" style="color:#004D97"><i class="fa fa-users"></i><span> Convocatoria </span> </a>
                        </li>



                    </ul>
                    <div class="clearfix"></div>
                </div>
                <!-- Sidebar -->
                <div class="clearfix"></div>

            </div>

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container-fluid">
                    <div class="row">

                        <div class="col-xs-12 col-sm-3">
                            <a asp-controller="Inscription" asp-action="Candidate" class="btn btn-info" style="color:#004D97"><i class="fa fa-address-book"></i> DATOS PERSONALES</a>
                        </div>

                        <div class="col-xs-12 col-sm-3">
                            <a asp-controller="Studies" asp-action="Studies" class="btn btn-success" style="color:#004D97"><i class="fa fa-graduation-cap"></i> FORMACIÓN</a>
                        </div>

                        <div class="col-xs-12 col-sm-3">
                            <a asp-controller="Experience" asp-action="Experience" class="btn btn-primary" style="color:#004D97"><i class="fa fa-black-tie"></i> EXPERIENCIA</a>
                        </div>

                        <div class="col-xs-12 col-sm-3">
                            <a asp-controller="Document" asp-action="Document" class="btn btn-danger" style="color:#004D97"><i class="fa fa-file-text"></i> OTROS DOCUMENTOS</a>
                        </div>

                        <div class="container-fluid">
                            <div class="page-header">
                                <h1 class="text-center"><small style="color:seagreen">@ViewData["Description"]</small></h1>
                            </div>
                        </div>
                    </div>

                    <div class="row">



                        @RenderBody()

                    </div>
                    <!-- end row -->

                </div> <!-- container -->

            </div> <!-- content -->

            <footer class="footer text-right">
                2019 © Pastoral Social Derechos Reservados
            </footer>

        </div>

    </div>
    <!-- END wrapper -->
    <!-- jQuery  -->
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/popper.js/umd/popper.js"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/Detect.js/detect.js"></script>
    <script src="~/lib/fastclick/fastclick.js"></script>
    <script src="~/lib/jquery.blockUI/jquery.blockUI.js"></script>
    <script src="~/lib/assets/waves.js"></script>
    <script src="~/lib/jquery.nicescroll/jquery.nicescroll.js"></script>
    <script src="~/lib/jQuery-slimScroll/jquery.slimscroll.js"></script>
    <script src="~/lib/jquery-scrollTo/jquery.scrollTo.min.js"></script>

    <!-- KNOB JS -->
    <!--[if IE]>
    <script type="text/javascript" src="assets/plugins/jquery-knob/excanvas.js"></script>
    <![endif]-->
    <script src="~/lib/jQuery-Knob/jquery.knob.min.js"></script>

    <!--Morris Chart-->
    <script src="~/lib/morris.js/morris.min.js"></script>

    <script src="~/lib/raphael/raphael.min.js"></script>

    <!-- Dashboard init -->
    <script src="~/lib/assets/jquery.dashboard.js"></script>


    <!-- App js -->
    <script src="~/lib/assets/jquery.core.js"></script>
    <script src="~/lib/assets/jquery.app.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
